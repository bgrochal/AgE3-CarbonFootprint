<CarbonFootprint>

	<Actions>
		<Action title="Amount of methane to catalyze" type="AmountOfMethaneToCatalyze" method="getAmountOfMethaneToCatalyze">
			<Parameters>
				<Parameter type="Range" name="Amount of methane [m^3]">
					<Range min="0.0" max="16.6"/>
				</Parameter>
			</Parameters>
		</Action>

		<Action title="Footprint for emission of pure methane" type="FootprintEmissionOfPureMethane" method="getFootprintEmissionOfPureMethane">
			<Parameters>
				<Parameter name="Maximum amount of methane [m^3]" value="16.6"/>
				<Parameter name="Methane to CO2 multiplier" value="25.0"/>
			</Parameters>
			<Footprints>
				<Footprint>AmountOfMethaneToCatalyze</Footprint>
			</Footprints>
		</Action>

		<Action title="Catalytic combustion of methane" type="CatalyticCombustionOfMethane" method="getCatalyticCombustionOfMethane">
			<Footprints>
				<Footprint>AmountOfMethaneToCatalyze</Footprint>
			</Footprints>
		</Action>

		<Action title="Percentage of CO2 emission generated by combustion" type="PercentageCo2Emissions" method="getPercentageCo2Emissions">
			<Parameters>
				<Parameter type="Range" name="Percent of CO2 emission">
					<Range min="0.0" max="1.0"/>
				</Parameter>
			</Parameters>
		</Action>

		<Action title="CO2 produced by combustion" type="Co2ProducedByCombustion" method="getCo2ProducedByCombustion">
			<Parameters>
				<Parameter name="Combustion multiplier" value="1.96"/>
			</Parameters>
			<Footprints>
				<Footprint>CatalyticCombustionOfMethane</Footprint>
			</Footprints>
		</Action>

		<Action title="Footprint for emission of as a result of combustion" type="FootprintEmissionCombustion" method="getFootprintEmissionCombustion">
			<Footprints>
				<Footprint>PercentageCo2Emissions</Footprint>
				<Footprint>Co2ProducedByCombustion</Footprint>
			</Footprints>
		</Action>

		<Action title="CO2 Separated" type="Co2Separated" method="getCo2Separated">
			<Footprints>
				<Footprint>PercentageCo2Emissions</Footprint>
				<Footprint>Co2ProducedByCombustion</Footprint>
			</Footprints>
		</Action>

		<Action title="Heat energy demand for SELEXOL" type="HeatEnergyDemandSelexol" method="getHeatEnergyDemandSelexol">
			<Parameters>
				<Parameter name="Energy multiplier" value="0.2238"/>
			</Parameters>
			<Footprints>
				<Footprint>Co2Separated</Footprint>
			</Footprints>
		</Action>

		<Action title="Cooling energy demand for SELEXOL" type="CoolingEnergyDemandSelexol" method="getCoolingEnergyDemandSelexol">
			<Parameters>
				<Parameter name="Energy multiplier" value="0.5590"/>
			</Parameters>
			<Footprints>
				<Footprint>Co2Separated</Footprint>
			</Footprints>
		</Action>

		<Action title="Electric energy demand for SELEXOL" type="ElectricEnergyDemandSelexol" method="getElectricEnergyDemandSelexol">
			<Parameters>
				<Parameter name="Energy multiplier" value="0.1080"/>
			</Parameters>
			<Footprints>
				<Footprint>Co2Separated</Footprint>
			</Footprints>
		</Action>

		<Action title="Electric energy demand for compression" type="ElectricEnergyDemandForCompression" method="getElectricEnergyDemandForCompression">
			<Parameters>
				<Parameter name="Energy multiplier" value="0.3"/>
			</Parameters>
			<Footprints>
				<Footprint>Co2Separated</Footprint>
			</Footprints>
		</Action>

		<Action title="Emission from the production of 1 MJ of heat energy" type="EmissionProductionHeatEnergy" method="getEmissionProductionHeatEnergy">
			<Parameters>
				<Parameter name="Emission from energy production" value="0.0635"/>
			</Parameters>
		</Action>

		<Action title="Emission from the production of 1 kWh of electric energy" type="EmissionProductionElectricEnergy" method="getEmissionProductionElectricEnergy">
			<Parameters>
				<Parameter name="Emission from energy production" value="0.9124"/>
			</Parameters>
		</Action>

		<Action title="Footprint for production of energy" type="FootprintEnergyProduction" method="getFootprintEnergyProduction">
			<Footprints>
				<Footprint>EmissionProductionHeatEnergy</Footprint>
				<Footprint>EmissionProductionElectricEnergy</Footprint>
				<Footprint>HeatEnergyDemandSelexol</Footprint>
				<Footprint>CoolingEnergyDemandSelexol</Footprint>
				<Footprint>ElectricEnergyDemandSelexol</Footprint>
				<Footprint>ElectricEnergyDemandForCompression</Footprint>
			</Footprints>
		</Action>

		<Action title="Overall footprint" type="OverallFootprint" method="getOverallFootprint">
			<Footprints>
				<Footprint>FootprintEmissionOfPureMethane</Footprint>
				<Footprint>FootprintEmissionCombustion</Footprint>
				<Footprint>FootprintEnergyProduction</Footprint>
			</Footprints>
		</Action>
	</Actions>

	<Target>OverallFootprint</Target>

</CarbonFootprint>
